<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Company>Mzying2001</Company>
    <Authors>Mzying2001</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Mzying2001.SimpleMvvm</PackageId>
    <Description>A simple MVVM class library for WPF apps.</Description>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Copyright>Copyright (c) 2021 Mzying2001</Copyright>
    <PackageProjectUrl>https://github.com/Mzying2001/SimpleMvvm</PackageProjectUrl>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>D:\code\VisualStudio\repos\SimpleMvvm\SimpleMvvm\SimpleMvvm.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>SimpleMvvm.xml</DocumentationFile>
    <NoWarn></NoWarn>
  </PropertyGroup>

</Project>
